#include <stdio.h>
  int main()
{
	double x[5];
	int i;
	char grade;
	i=0;
    for (i=0;i<5;i++)
    {
  	scanf ("%lf",&x[i]);
    }
    for (i=0;i<5;i++)
    {
  	if(x[i]<0||x[i]>100)
	  printf ("Error\n");
	else
	 { 
	  switch ((int)x[i]/10)
	   {
		case 9:grade='A';break;
		case 8:grade='B';break;
		case 7:grade='C';break;
		case 6:grade='D';break;
		default:grade='E';
	    }
	   printf ("%c\n",grade);
       }
   }
	return 0;
   
}
   
