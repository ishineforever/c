#include <stdio.h>
  int main()
{
	int x,y;
	char grade;
	scanf ("%d",&x);
	if(x<0||x>100)
	  printf ("Error");
    else
	switch (x/10)
	{
		case 9:grade='A';break;
		case 8:grade='B';break;
		case 7:grade='C';break;
		case 6:grade='D';break;
		default:grade='E';
	}
    printf ("%c",grade); 
	return 0;
   
}
   
