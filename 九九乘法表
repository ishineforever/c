#include <stdio.h>

  int main ()
  {
    int i,j,n,m;
    n>0&&n<10;
    m=2;
    while(m!=0)
    {
         scanf ("%d",&n);
         for (i=1;i<=n;i++)
         {
    	      for (j=1;j<=9;j++)
    	      {
    	           if (j>i)
	                   printf (" ");
	                else
                       printf ("%d*%d=%4d\t",i,j,i*j);
              }
         printf ("\n");
 	     }
 	     m--;
        
	}
    return 0;  
  }
